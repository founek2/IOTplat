{
  "name": "iot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "updateFrontEnd": "npm run build --prefix ../frontend && rm -rf public/* && cp -R ../frontend/build/* public/",
    "deploy": "env NODE_ENV=production npm run updateFrontEnd && npm start"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "hat": "0.0.3",
    "jade": "~1.11.0",
    "moment": "^2.21.0",
    "mongoose": "^5.0.10",
    "morgan": "~1.9.0",
    "node-fetch": "^2.1.2",
    "ramda": "^0.25.0",
    "serve-favicon": "~2.4.5"
  }
}
